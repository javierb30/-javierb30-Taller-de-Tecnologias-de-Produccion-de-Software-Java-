<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" version="5.0">
  <context-param>
    <param-name>abono2Dias</param-name>
    <param-value>5</param-value>
  </context-param>
  <context-param>
    <param-name>cantVendidosAbono2Dias</param-name>
    <param-value>0</param-value>
  </context-param>
  <context-param>
    <param-name>abono3Dias</param-name>
    <param-value>10</param-value>
  </context-param>
  <context-param>
    <param-name>cantVendidosAbono3Dias</param-name>
    <param-value>0</param-value>
  </context-param>
  <listener>
    <listener-class>misListeners.ContadorAbonos</listener-class>
  </listener>
  <display-name>TTPS2024_ENTREGABLE1_Ballesteros</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
